@{
  Layout = null;
}
<!DOCTYPE html>
<html ng-app="myApp">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Quiz Admin</title>

  <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

  <!-- Optional theme -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">

  <!-- JQuery 2.x for Bootstrap JS Modal Support -->
  <script src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>

  <!-- Latest compiled and minified JavaScript -->
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

  <!-- Personal CSS -->
  <link href="~/CSS/globalStyles.css" rel="stylesheet" />
  <link href="~/CSS/welcomePage.css" rel="stylesheet" />
  <link href="~/CSS/signUp.css" rel="stylesheet" />
  <link href="~/CSS/homeView.css" rel="stylesheet" />
  <link href="~/CSS/takeQuiz.css" rel="stylesheet" />
  <link href="~/CSS/animate.css" rel="stylesheet" />
</head>
<body ng-cloak">

  <div ui-view onload="answerLetterHoverColor()"></div>

  <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.3/angular.js"></script>
  <!-- UI Router -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.1/angular-ui-router.js"></script>

    <!--
        You must add the bundle for the angular by hand
        Just paste:
            bundles.Add(new ScriptBundle("~/bundles/angular-app").Include("~/Scripts/app/*.js"));
        right into BundleConfig.cs
    -->
    <script src="~/Scripts/TakeQuiz/modules/app.js"></script>
    <script src="~/Scripts/TakeQuiz/controllers/takeQuizController.js"></script>
    <script src="~/Scripts/TakeQuiz/controllers/homeViewController.js"></script>
    <script src="~/Scripts/TakeQuiz/controllers/defaultController.js"></script>
    <script src="~/Scripts/TakeQuiz/controllers/signUpController.js"></script>
    <script src="~/Scripts/app/UserControllers.js"></script>
    <script src="~/Scripts/app/QuizControllers.js"></script>
    <script src="~/Scripts/app/QuestionControllers.js"></script>
    <script src="~/Scripts/app/QuizRunControllers.js"></script>
    <script src="~/Scripts/app/AnswerControllers.js"></script>

</body>
</html>
