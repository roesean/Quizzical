@{
    Layout = null;
}
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a href="#/" class="navbar-brand">Admin</a>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                @*Hardcode this section to be your own custom nav bar using ui-sref*@
                <li>
                    <a ui-sref="userList" class="navbar-brand">User</a>
                </li>
                <li>
                    <a ui-sref="quizList" class="navbar-brand">Quiz</a>
                </li>
                <li>
                    <a ui-sref="questionList" class="navbar-brand">Question</a>
                </li>
                <li>
                    <a ui-sref="answerList" class="navbar-brand">Answer</a>
                </li>
                <li>
                    <a ui-sref="Default" class="navbar-brand">Take Quiz</a>
                </li>
            </ul>
        </div>
    </div>
</div>
<div class="divBelowNav container">
    <h2>QuizRun</h2>
    <form name="theForm">
        <div class="form-horizontal">
            <ul id="validationSummary"></ul>
            <div class="form-group">
                <label for="Score" class="control-label col-md-2"> Score </label>
                <div class="col-md-10">
                    <input type="number" ng-model="data.Score" id="Score" class="form-control" />
                    <div id="validation_for_Score"></div>
                </div>
            </div>

            <div class="form-group">
                <label for="DateTaken" class="control-label col-md-2"> DateTaken </label>
                <div class="col-md-10">
                    <input type="datetime" ng-model="data.DateTaken" id="DateTaken" class="form-control" />
                    <div id="validation_for_DateTaken"></div>
                </div>
            </div>

            <div class="form-group">
                <label for="UserId" class="control-label col-md-2"> User </label>
                <div class="col-md-10">
                    <select ng-model="data.UserId" id="UserId" class="form-control"
                            ng-options="option.Id as option.Name for option in UserId_options">
                        <option value="">-- choose User --</option>
                    </select>
                    <div id="validation_for_UserId"></div>
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="button" value="Save" class="btn btn-default" ng-click="save()" />
                </div>
            </div>
        </div>
    </form>

    <div>
        <a href="#/QuizRun">Back to List</a>
    </div>
</div>